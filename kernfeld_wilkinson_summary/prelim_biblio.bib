	Eprint = {http://biostatistics.oxfordjournals.org/content/early/2014/01/06/biostatistics.kxt053.full.pdf+html},
	Journal = {Biostatistics},
	Title = {Differential expression analysis of RNA-seq data at single-base resolution},
	Url = {http://biostatistics.oxfordjournals.org/content/early/2014/01/06/biostatistics.kxt053.abstract},
	Year = {2014},
