	Doi = {10.1109/ICDM.2006.160},
	Issn = {1550-4786},
	Keywords = {matrix decomposition;pattern clustering;clustering;interpretation;nonnegative matrix factorization methods;normalization;Algorithm design and analysis;Clustering algorithms;Clustering methods;Computer science;DNA;Data mining;Matrix decomposition;Pattern recognition;Principal component analysis;Text mining;matrix factorization;normalization;relation;simultaneous clustering},
