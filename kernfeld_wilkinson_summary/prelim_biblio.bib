@article{Yu01042012,
	Abstract = {There has been a sustained need to quantify past climate changes from proxy records to better understand the driving mechanisms and thus to improve the prediction of the future. Transfer function is an intuitive and frequently used method in this regard. However, this method is unable to provide interpretive and predictive results from proxy records, because physical processes are not included. The inverse proxy modelling method opened up a new avenue for quantitative palaeoclimate reconstructions through the integration of proxy records with deterministic models. It is fundamentally different from the statistical approach, representing a conceptual advance in quantitative palaeoclimatology. Here we demonstrate the potential of this method by placing a mechanistic model and a 6000 year long peat cellulose δ18O record obtained from the high-cold and monsoonal eastern Tibetan Plateau (c. 3500 m a.s.l.) in a Bayesian paradigm. In this worked example, the marginal posterior probability distributions of palaeoclimate variables such as the δ18O of soil water, temperature, and relative humidity were inferred jointly through the solution to an ill-posed inverse problem using the Markov chain-Monte Carlo method. Our results indicate that the observed variation of the peat cellulose δ18O record in this monsoonal area essentially reflects the changes in the oxygen isotopic composition of soil water, which is closely linked to that of rainfall. Compared with hydrology, temperature and humidity have little influence on the oxygen isotope fractionation of leaf water.},
	Author = {Yu, Shi-Yong and Kang, Zhihai and Zhou, Weijian},
	Date-Added = {2014-02-25 18:59:22 +0000},
	Date-Modified = {2014-02-25 18:59:22 +0000},
	Doi = {10.1177/0959683611425544},
	Eprint = {http://hol.sagepub.com/content/22/4/405.full.pdf+html},
	Journal = {The Holocene},
	Number = {4},
	Pages = {405-412},
	Title = {Quantitative palaeoclimate reconstruction as an inverse problem: A Bayesian inference of late-Holocene climate on the eastern Tibetan Plateau from a peat cellulose δ18O record},
	Url = {http://hol.sagepub.com/content/22/4/405.abstract},
	Volume = {22},
	Year = {2012},
	Bdsk-Url-1 = {http://hol.sagepub.com/content/22/4/405.abstract},
	Bdsk-Url-2 = {http://dx.doi.org/10.1177/0959683611425544}}

@article{Yu:2013pb,
	Annote = {10.1177/0959683613496292},
	Author = {Yu, Shi-Yong},
	Date = {2013/11/01},
	Date-Added = {2014-02-25 18:58:26 +0000},
	Date-Modified = {2014-02-25 18:58:26 +0000},
	Doi = {10.1177/0959683613496292},
	Journal = {The Holocene},
	M3 = {10.1177/0959683613496292},
	Month = {11},
	Number = {11},
	Pages = {1507--1516},
	Title = {Quantitative reconstruction of mid- to late-Holocene climate in NE China from peat cellulose stable oxygen and carbon isotope records and mechanistic models},
	Ty = {JOUR},
	Url = {http://hol.sagepub.com/content/23/11/1507.abstract N2 - There has been an increasing need to reconstruct past climate from proxy records quantitatively and mechanistically. The inverse proxy modeling method stands out as a novel approach to quantitative palaeoclimate reconstructions through integrating process-based models and proxy records, representing a major progress in quantitative palaeoclimatology. It has been proposed to incorporate multiple proxy records to produce a more robust constraint on the climate parameters sought for estimation, and most of the work has been conducted using pollen records in conjunction with vegetation models. Here, I show a worked example of using paired stable oxygen and carbon isotope records of peat cellulose from one single core to infer the climate history in NE China for the last 6000 years through solving a well-posed inverse problem using Bayesian statistics. The quantitative palaeoclimate data obtained in this study may deepen our insight into the dynamics of the East Asian summer monsoon. Mean growing season temperature and relative humidity show millennial-scale fluctuations prior to c. 4000 cal. yr BP; thereafter, centennial-scale fluctuations prevailed, revealing the relative importance of solar activity over tropical ocean–atmosphere interactions in regulating the variability of regional climate during the late Holocene. It appears that there was a prominent out-of-phase relationship between temperature and relative humidity, due probably to the different response of these climate elements to orbital forcing and land cover. This worked example demonstrates the potential of using model–data fusion techniques to produce physically meaningful, mathematically optimal, and geologically sound results.},
	Volume = {23},
	Year = {2013},
	Bdsk-Url-1 = {http://hol.sagepub.com/content/23/11/1507.abstract%20N2%20-%20There%20has%20been%20an%20increasing%20need%20to%20reconstruct%20past%20climate%20from%20proxy%20records%20quantitatively%20and%20mechanistically.%20The%20inverse%20proxy%20modeling%20method%20stands%20out%20as%20a%20novel%20approach%20to%20quantitative%20palaeoclimate%20reconstructions%20through%20integrating%20process-based%20models%20and%20proxy%20records,%20representing%20a%20major%20progress%20in%20quantitative%20palaeoclimatology.%20It%20has%20been%20proposed%20to%20incorporate%20multiple%20proxy%20records%20to%20produce%20a%20more%20robust%20constraint%20on%20the%20climate%20parameters%20sought%20for%20estimation,%20and%20most%20of%20the%20work%20has%20been%20conducted%20using%20pollen%20records%20in%20conjunction%20with%20vegetation%20models.%20Here,%20I%20show%20a%20worked%20example%20of%20using%20paired%20stable%20oxygen%20and%20carbon%20isotope%20records%20of%20peat%20cellulose%20from%20one%20single%20core%20to%20infer%20the%20climate%20history%20in%20NE%20China%20for%20the%20last%206000%20years%20through%20solving%20a%20well-posed%20inverse%20problem%20using%20Bayesian%20statistics.%20The%20quantitative%20palaeoclimate%20data%20obtained%20in%20this%20study%20may%20deepen%20our%20insight%20into%20the%20dynamics%20of%20the%20East%20Asian%20summer%20monsoon.%20Mean%20growing%20season%20temperature%20and%20relative%20humidity%20show%20millennial-scale%20fluctuations%20prior%20to%20c.%204000%20cal.%20yr%20BP;%20thereafter,%20centennial-scale%20fluctuations%20prevailed,%20revealing%20the%20relative%20importance%20of%20solar%20activity%20over%20tropical%20ocean%E2%80%93atmosphere%20interactions%20in%20regulating%20the%20variability%20of%20regional%20climate%20during%20the%20late%20Holocene.%20It%20appears%20that%20there%20was%20a%20prominent%20out-of-phase%20relationship%20between%20temperature%20and%20relative%20humidity,%20due%20probably%20to%20the%20different%20response%20of%20these%20climate%20elements%20to%20orbital%20forcing%20and%20land%20cover.%20This%20worked%20example%20demonstrates%20the%20potential%20of%20using%20model%E2%80%93data%20fusion%20techniques%20to%20produce%20physically%20meaningful,%20mathematically%20optimal,%20and%20geologically%20sound%20results.},
	Bdsk-Url-2 = {http://dx.doi.org/10.1177/0959683613496292}}

